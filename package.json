{
  "name": "vue-form-utils",
  "version": "0.1.0",
  "description": "A collection of form utilities for working with forms in Vue",
  "author": "matt@brophy.org",
  "license": "MIT",
  "main": "dist/vue-form-utils.umd.js",
  "module": "dist/vue-form-utils.esm.js",
  "unpkg": "dist/vue-form-utils.min.js",
  "browser": "src/index.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brophdawg11/vue-form-utils.git"
  },
  "bugs": {
    "url": "https://github.com/brophdawg11/vue-form-utils/issues"
  },
  "scripts": {
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "cross-env NODE_ENV=production rollup --config rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config rollup.config.js --format iife",
    "clean": "rimraf dist/ && rimraf storybook-static/ && rimraf cypress/",
    "lint": "eslint --ext js,vue .",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "2.*"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@rollup/plugin-buble": "0.21.0",
    "@rollup/plugin-commonjs": "11.0.1",
    "@rollup/plugin-replace": "2.3.0",
    "@storybook/addon-actions": "5.3.6",
    "@storybook/addon-links": "5.3.6",
    "@storybook/addons": "5.3.6",
    "@storybook/vue": "5.3.6",
    "@vue/test-utils": "1.0.0-beta.30",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "6.0.3",
    "cypress": "3.8.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-vue": "6.1.2",
    "husky": "4.0.10",
    "jest": "24.9.0",
    "minimist": "1.2.0",
    "rimraf": "3.0.0",
    "rollup": "1.29.0",
    "rollup-plugin-terser": "5.2.0",
    "rollup-plugin-vue": "5.1.5",
    "vue": "2.6.11",
    "vue-jest": "3.0.5",
    "vue-loader": "15.8.3",
    "vue-template-compiler": "2.6.11"
  }
}
